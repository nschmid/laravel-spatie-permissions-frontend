{
    "name": "webdevhayes/laravel-spatie-permissions-frontend",
    "description": "Frontend boilerplate for spatie permissions package",
    "keywords": [
        "webdevhayes",
        "laravel-spatie-permissions-frontend"
    ],
    "homepage": "https://github.com/nschmid/laravel-spatie-permissions-frontend",
    "license": "MIT",
    "authors": [
        {
            "name": "James",
            "email": "hayesj89@hotmail.co.uk",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/filesystem": "^8.0",
        "illuminate/support": "^8.0|^9.0",
        "spatie/laravel-permission": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Webdevhayes\\LaravelSpatiePermissionsFrontend\\": "src",
            "Webdevhayes\\LaravelSpatiePermissionsFrontend\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webdevhayes\\LaravelSpatiePermissionsFrontend\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webdevhayes\\LaravelSpatiePermissionsFrontend\\LaravelSpatiePermissionsFrontendServiceProvider"
            ],
            "aliases": {
                "LaravelSpatiePermissionsFrontend": "Webdevhayes\\LaravelSpatiePermissionsFrontend\\LaravelSpatiePermissionsFrontendFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
